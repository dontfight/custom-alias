#Set PHP path to php 7
export PATH="$(brew --prefix homebrew/php/php71)/bin:$PATH"

#custom alias
alias dkc='docker-compose'
alias dpi="docker images |grep -v REPOSITORY|awk '{print \$1}'|xargs -L1 docker pull"
alias sqlyog='/usr/local/bin/wine ~/.wine/drive_c/Program\ Files\ \(x86\)/SQLyog/SQLyog.exe'
alias pstorm='open -a PhpStorm'

#Remove autocomplete suggestion of ORIG_HEAD
zstyle ':completion:*:*' ignored-patterns '*ORIG_HEAD'
